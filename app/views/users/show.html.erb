<h3><%= @user.name %></h3>

<hr>

<div id="pins" class="transitions-enabled infinite-scroll centered clearfix">

  <% @user.pins.each do |pin| %>
  <div class="box panel panel-default">

    <% if pin.image.attached? %>
      <%= link_to (image_tag(pin.image)), pin %>
    <% else %>
    <h2 class="pin-link">
      <%= link_to pin.title, pin %>
    </h2>
    <% end %>


  </div>
  <% end %>
</div>
